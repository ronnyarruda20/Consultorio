
type Query {
	#Paciente query
	consultarPacientePorId(id: Long!): Paciente
	#Paciente query
	consultarTodosPacientes:[Paciente]
}

type Mutation {
	#Paciente mutation
	incluirPaciente(pacienteDTO: PacienteDTO!): Boolean
	excluirPaciente(id: Long!):Boolean
}

input PacienteDTO {
	id:Long
	nome:String
	dataNascimento:String
	profissoes:[Long]
}

type Paciente {
	id:ID
	nome:String
	dataNascimento:String
	profissoes:[Profissao]
}

type Profissao {
	id: ID
	codigo: String
	descricao: String

}